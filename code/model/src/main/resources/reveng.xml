<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
    <table-filter match-name="spatial_ref_sys" exclude="true"/>
    <table-filter match-name="jbpm.*" exclude="true"/>
    <table-filter match-name="geometry_columns" exclude="true"/>
    <table-filter match-name="zipcodes" exclude="true"/>
    <table-filter match-name="geo_point" exclude="true"/>
    <table-filter match-name="state_lookup" exclude="true"/>
    <table-filter match-name="activemq.*" exclude="true"/>
    <table-filter match-name="ftp_user_view" exclude="true"/>
</hibernate-reverse-engineering>
